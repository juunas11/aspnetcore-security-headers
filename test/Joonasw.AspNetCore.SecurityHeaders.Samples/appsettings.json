{
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Hsts": {
        "Duration": "30:00:00:00",
        "IncludeSubDomains": false,
